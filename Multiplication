import java.util.*;

public class Multiplication {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int N = scanner.nextInt();
        
        for (int i = 1; i <= 9; i++) {
            int result = N * i;
            System.out.println(N + " * " + i + " = " + result);
        }
    }
}
